<!-- ))))))))))))))))))))))))))) scoreentrybackground ((((((((((((((((((((((((((
     This is the background file for a normal score in the Score page. The score
     entry is displayed with a blue background and white border.  
-->
<shape 
    xmlns:android="http://schemas.android.com/apk/res/android" 
    android:shape="rectangle"
>
    <!-- blue background -->
    <solid 
        android:color="@color/mapTurquoise"
    />
    
    <!-- white border -->
    <stroke
        android:width=".75dip"
        android:color="@color/white"
    />
    
    <!-- slightly rounded corners -->
    <corners
       android:radius="3dip"        
    />

</shape>